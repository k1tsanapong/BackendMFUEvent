{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "Event",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "dateTimeStart": {
      "type": "datetime",
      "required": true
    },
    "dateTimeEnd": {
      "type": "datetime",
      "required": false
    },
    "school": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::school.school",
      "inversedBy": "events"
    },
    "limit": {
      "type": "integer"
    },
    "contact": {
      "type": "string",
      "required": true
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "inversedBy": "events"
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "cover": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    }
  }
